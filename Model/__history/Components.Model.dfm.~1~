  end
  object qryProduct: TFDQuery
    Connection = FDConnection
    SQL.Strings = (
      
        'select P.*, G.DESCRICAO as NGRUPO, S.DESCRICAO as NSUBGRUPO, S.T' +
        'IPO as SUBTIPO, S.PONTAESTOQUE as SUBPONTAESTOQUE,'
      'G.RESUMO as GRUPORESUMO, G.ORDEM as GRUPOORDEM from'
      'PRODUTO P'
      'inner join GRUPO G on (G.CODGRUPO = P.CODGRUPO)'
      'inner join SUBGRUPO S on (S.CODSUBGRUPO = P.CODSUBGRUPO)'
      
        'where (P.SITUACAO = 1) and (S.ativosite = '#39'S'#39' or G.ativosite = '#39 +
        'S'#39')'
      ''
      '')
    Left = 175
    Top = 34
    object qryProductREFERENCIA: TStringField
